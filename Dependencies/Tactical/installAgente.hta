<html>
<head>
<meta charset="utf-8"/>
<style>
    body{
        background-color: rgb(33, 33, 33);
        color: "white";
    }
    #Bioprev{
        width: 100%;
	    height: 100%;
    }
</style>
<title>Inventario</title>

<HTA:APPLICATION
    APPLICATIONNAME="Agente"
    ICON="../Config/ti-tools-agente.ico"
    SCROLL="no"
    ID="Agente"
    VERSION="1.0"/>
</head>

<script language="VBScript">
    Sub Window_OnLoad
        window.resizeto 300,740
    End Sub

    const breakLine = "_____________________________________________________________________________________________________________________________"
    dim client
    dim site

    Set oShell = CreateObject ("WScript.Shell")
    Sub checkAgente

        if checkAgent.Value = "Bioprev_Douradina" Then
            client = "5"
            site = "8"
        Elseif checkAgent.Value = "Bioprev_Goioere" Then
            client = "5"
            site = "9"
        Elseif checkAgent.Value = "Bioprev_NovaOlimpia" Then
            client = "5"
            site = "10"
        Elseif checkAgent.Value = "Bioprev_Umuarama" Then
            client = "5"
            site = "5"

        Elseif checkAgent.Value = "Clinica_Cacoal-RO" Then
            client = "3"
            site = "13"
        Elseif checkAgent.Value = "Clinica_Cacascavel" Then
            client = "3"
            site = "11"
        Elseif checkAgent.Value = "Clinica_CidadeGaucha" Then
            client = "3"
            site = "14"
        Elseif checkAgent.Value = "Clinica_STerezinhaDoOeste" Then
            client = "3"
            site = "12"
        Elseif checkAgent.Value = "Clinica_Toledo" Then
            client = "3"
            site = "15"
        Elseif checkAgent.Value = "Clinica_Umuarama" Then
            client = "3"
            site = "3"

        Elseif checkAgent.Value = "Farmacia_Altonia" Then
            client = "2"
            site = "20"
        Elseif checkAgent.Value = "Farmacia_AssisChateaubriand" Then
            client = "2"
            site = "18"
        Elseif checkAgent.Value = "Farmacia_Cacoal-RO" Then
            client = "2"
            site = "21"
        Elseif checkAgent.Value = "Farmacia_Cascavel" Then
            client = "2"
            site = "16"
        Elseif checkAgent.Value = "Farmacia_Corbelia" Then
            client = "2"
            site = "19"
        Elseif checkAgent.Value = "Farmacia_CruzeiroDoOeste" Then
            client = "2"
            site = "23"
        Elseif checkAgent.Value = "Farmacia_Goioere" Then
            client = "2"
            site = "24"
        Elseif checkAgent.Value = "Farmacia_Guaira" Then
            client = "2"
            site = "22"
        Elseif checkAgent.Value = "Farmacia_Tapira" Then
            client = "2"
            site = "17"
        Elseif checkAgent.Value = "Clinica_Umuarama" Then
            client = "2"
            site = "2"

        Elseif checkAgent.Value = "Memorial_Douradina" Then
            client = "6"
            site = "33"
        Elseif checkAgent.Value = "Memorial_Toledo" Then
            client = "6"
            site = "29"
        Elseif checkAgent.Value = "Memorial_Umuarama" Then
            client = "6"
            site = "7"

        Elseif checkAgent.Value = "Plano_Cacoal-RO" Then
            client = "1"
            site = "28"
        Elseif checkAgent.Value = "Plano_Cascavel" Then
            client = "1"
            site = "6"
        Elseif checkAgent.Value = "Plano_Corbelia" Then
            client = "1"
            site = "27"
        Elseif checkAgent.Value = "Plano_Guarapuava" Then
            client = "1"
            site = "26"
        Elseif checkAgent.Value = "Plano_Toledo" Then
            client = "1"
            site = "25"
        Elseif checkAgent.Value = "Plano_Umuarama" Then
            client = "1"
            site = "1"

        Elseif checkAgent.Value = "Generico_Realocar" Then
            client = "8"
            site = "32"
        End if


        if checkAgent.Value = "" Then
            Window.Alert "Escolha uma Região!"
        Else
            oShell.Run "powershell.exe -Executionpolicy Bypass -nologo -noninteractive -file \\000.000.000.000\Tactical\Agente\Agente.ps1 -local "+checkAgent.Value+" -client "+client+" -site "+site+" "

            oShell.Run "cmd.exe /C date /t >> \\000.000.000.000\Tactical\Inventario_Logs\%username%.txt"
            oShell.Run "cmd.exe /C time /t >> \\000.000.000.000\Tactical\Inventario_Logs\%username%.txt"
            oShell.Run "cmd.exe /C echo "+checkAgent.Value+" >> \\000.000.000.000\Tactical\Inventario_Logs\%username%.txt"

            oShell.Run "cmd.exe /C echo Install Sucessfull! >> \\000.000.000.000\Tactical\Inventario_Logs\%username%.txt"
            oShell.Run "cmd.exe /C echo "+breakLine+" >> \\000.000.000.000\Tactical\Inventario_Logs\%username%.txt"
        End if

    End Sub

</script>

<body>
    <div id="container">

        <Center>
            <div id="Bioprev">
                <h4>Selecione uma Região</h4>
                <select size="36" id="teste" name="checkAgent">

                    <optgroup label="Bioprev">
                        <option onChange="teste" id="options" value="Bioprev_Douradina">&nbsp&nbsp&nbsp Douradina &nbsp&nbsp&nbsp</option>
                        <option value="Bioprev_Goioere">&nbsp&nbsp&nbsp Goioere &nbsp&nbsp&nbsp</option>
                        <option value="Bioprev_NovaOlimpia">&nbsp&nbsp&nbsp Nova Olimpia &nbsp&nbsp&nbsp</option>
                        <option value="Bioprev_Umuarama">&nbsp&nbsp&nbsp Umuarama &nbsp&nbsp&nbsp</option>
                    </optgroup>

                    <optgroup label="Clinica">
                        <option id="options" value="Clinica_Cacoal-RO">&nbsp&nbsp&nbsp Cacoal-RO &nbsp&nbsp&nbsp</option>
                        <option value="Clinica_Cacascavel">&nbsp&nbsp&nbsp Cascavel &nbsp&nbsp&nbsp</option>
                        <option value="Clinica_CidadeGaucha">&nbsp&nbsp&nbsp Cidade Gaucha &nbsp&nbsp&nbsp</option>
                        <option value="Clinica_STerezinhaDoOeste">&nbsp&nbsp&nbsp Sant Terezinha D/Oeste &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Clinica_Toledo">&nbsp&nbsp&nbsp Toledo &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Clinica_Umuarama">&nbsp&nbsp&nbsp Umuarama &nbsp&nbsp&nbsp</option>
                    </optgroup>

                    <optgroup label="Farmacia">
                        <option id="options" value="Farmacia_Altonia">&nbsp&nbsp&nbsp Altonia &nbsp&nbsp&nbsp</option>
                        <option value="Farmacia_AssisChateaubriand">&nbsp&nbsp&nbsp Assis Chateuabriand &nbsp&nbsp&nbsp</option>
                        <option value="Farmacia_Cacoal-RO">&nbsp&nbsp&nbsp Cacoal-RO &nbsp&nbsp&nbsp</option>
                        <option value="Farmacia_Cascavel">&nbsp&nbsp&nbsp Cascavel &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Farmacia_Corbelia">&nbsp&nbsp&nbsp Corbelia &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Farmacia_CruzeiroDoOeste">&nbsp&nbsp&nbsp Cruzeiro do Oeste &nbsp&nbsp&nbsp</option>
                        <option value="Farmacia_Goioere">&nbsp&nbsp&nbsp Goioere &nbsp&nbsp&nbsp</option>
                        <option value="Farmacia_Guaira">&nbsp&nbsp&nbsp Guaira &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Farmacia_Tapira">&nbsp&nbsp&nbsp Tapira &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Farmacia_Umuarama">&nbsp&nbsp&nbsp Umuarama &nbsp&nbsp&nbsp</option>
                    </optgroup>

                    <optgroup label="Memorial">
                        <option id="options" value="Memorial_Douradina">&nbsp&nbsp&nbsp Douradina &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Memorial_Toledo">&nbsp&nbsp&nbsp Toledo &nbsp&nbsp&nbsp</option>
                        <option id="options" value="Memorial_Umuarama">&nbsp&nbsp&nbsp Umuarama &nbsp&nbsp&nbsp</option>
                    </optgroup>

                    <optgroup label="Plano">
                        <option value="Plano_Cacoal-RO"> &nbsp&nbsp&nbsp Cacoal-RO &nbsp&nbsp&nbsp </option>
                        <option value="Plano_Cascavel"> &nbsp&nbsp&nbsp Cascavel &nbsp&nbsp&nbsp </option>
                        <option value="Plano_Corbelia"> &nbsp&nbsp&nbsp Corbelia &nbsp&nbsp&nbsp </option>
                        <option value="Plano_Guarapuava"> &nbsp&nbsp&nbsp Guarapuava &nbsp&nbsp&nbsp </option>
                        <option value="Plano_Toledo"> &nbsp&nbsp&nbsp Toledo &nbsp&nbsp&nbsp </option>
                        <option value="Plano_Umuarama"> &nbsp&nbsp&nbsp Umuarama &nbsp&nbsp&nbsp </option>
                    </optgroup>

                    <optgroup label="Generico">
                        <option id="options" value="Generico_Realocar">&nbsp&nbsp&nbsp Realocar &nbsp&nbsp&nbsp</option>
                    </optgroup>

                </select>
            </div>
            <BR/>            
            <input type="button" onClick="checkAgente" value="instalar" style="cursor: pointer">
        </div>
    </center>

</body>

</html>